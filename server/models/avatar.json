{
  "name": "Avatar",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true,
      "default": "John"
    },
    "face": {
      "type": "Face"
    }
  },
  "validations": [],
  "relations": {
    "faces": {
      "type": "hasOne",
      "model": "Face",
      "foreignKey": ""
    },
    "hairs": {
      "type": "hasOne",
      "model": "Hair",
      "foreignKey": ""
    },
    "bodies": {
      "type": "hasOne",
      "model": "Body",
      "foreignKey": ""
    },
    "bottoms": {
      "type": "hasOne",
      "model": "Bottom",
      "foreignKey": ""
    },
    "generals": {
      "type": "hasOne",
      "model": "General",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {
    "render": {
      "accepts": [
        {
          "arg": "avatar",
          "type": "Avatar",
          "required": true,
          "http": {
            "source": "body"
          },
          "description": "Make your avatar!"
        }
      ],
      "returns": {
        "arg": "data",
        "type": "Avatar",
        "root": true
      }
    }
  }
}
